{
  "name": "@a3s-lab/lane",
  "version": "0.4.0",
  "description": "Node.js bindings for a3s-lane priority command queue",
  "main": "index.js",
  "types": "index.d.ts",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/A3S-Lab/Lane",
    "directory": "sdk/node"
  },
  "napi": {
    "name": "a3s-lane",
    "triples": {
      "defaults": true,
      "additional": [
        "aarch64-apple-darwin",
        "aarch64-unknown-linux-gnu",
        "aarch64-unknown-linux-musl"
      ]
    }
  },
  "optionalDependencies": {
    "@a3s-lab/lane-win32-x64-msvc": "0.4.0",
    "@a3s-lab/lane-darwin-x64": "0.4.0",
    "@a3s-lab/lane-darwin-arm64": "0.4.0",
    "@a3s-lab/lane-linux-x64-gnu": "0.4.0",
    "@a3s-lab/lane-linux-x64-musl": "0.4.0",
    "@a3s-lab/lane-linux-arm64-gnu": "0.4.0",
    "@a3s-lab/lane-linux-arm64-musl": "0.4.0"
  },
  "devDependencies": {
    "@napi-rs/cli": "^2.18.0"
  },
  "scripts": {
    "build": "napi build --platform --release",
    "prepublishOnly": "napi prepublish -t npm",
    "artifacts": "napi artifacts",
    "version": "napi version"
  }
}
